@page
@model SemesterProjekt_2.Pages.Members.UpdateMemberModel
@{
    ViewData["title"] = "UpdateMember";
}

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="@Model.Member.Name"></label>
                <input asp-for="@Model.Member.Name" class="form-control" />

            </div>
            <div class="form-group">
                <label asp-for="@Model.Member.Address"></label>
                <input asp-for="@Model.Member.Address" />

            </div>
            <div class="form-group">
                <label asp-for="@Model.Member.Email"></label>
                <input asp-for="@Model.Member.Email" />

            </div>
            <div class="form-group">
                <label asp-for="@Model.Member.IsFamily"></label>
                <input asp-for="@Model.Member.IsFamily" />
            </div>
            <div class="form-group">
                <a asp-page="GetAllMembers" class=" btn-btn-primary">Cancel</a>
                <input type="submit" value=" Add Member" class="btn-btn-primary" />
            </div>

        </form>
    </div>
</div>
