@page
@model SemesterProjekt_2.Pages.Medlemmer.GetAllMembersModel
@{
    ViewData["title"] = "GetAllMembersModel";
}

<table class="table">
    <thead>
        <tr>
            <th>
                MemberID
            </th>
            <th>
                Name
            </th>
            <th>
                Address
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Members)
        {
            <tr>
                <td>
                    @item.MedlemID
                </td>
                <td>
                    @item.Name
                </td>
                <td>
                    @item.Address
                </td>
                <td>
                    @item.Email
                </td>
            </tr>
        }
    </tbody>
</table>